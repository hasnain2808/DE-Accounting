 "html": "<!DOCTYPE html>\n<img src=\"/files/banner-916673_1920.jpg\" style=\"width: 100%;\">\n<h1 class=\"text-right\">SALES INVOICE</h1>\n<div class=\"text-right\">{{doc.name}}</div>\n<hr size=\"10\">\n\n<div class=\"row\">\n\t<div class=\"col-xs-3 \">Customer Name</div>\n\t<div class=\"col-xs-3\">{{doc.customer}}</div>\n\n\t<div class=\"col-xs-3\">Date</div>\n\t<div class=\"col-xs-3\">{{ doc.posting_date }}</div>\n</div>\n<div class = \"row\">\n<div class=\"col-xs-3\">Company Name</div>\n<div class=\"col-xs-3\">{{doc.company}}</div>\n</div>\n<table class=\"table table-bordered\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<th>Sr</th>\n\t\t\t<th>Product</th>\n\t\t\t<th class=\"text-right\">Qty</th>\n\t\t\t<th class=\"text-right\">Price</th>\n\t\t\t<th class=\"text-right\">Amount</th>\n\t\t</tr>\n\t\t{%- for row in doc.product_list -%}\n\t\t<tr>\n\t\t\t<td style=\"width: 3%;\">{{ row.idx }}</td>\n\n\t\t\t<td style=\"width: 37%;\">{{ row.product }}</td>\n\n\t\t\t<td style=\"width: 20%; text-align: right;\">{{ row.qty }}</td>\n\t\t\t<td style=\"width: 20%; text-align: right;\">{{\n\t\t\t\trow.get_formatted(\"selling_price\", doc) }}</td>\n\t\t\t<td style=\"width: 20%; text-align: right;\">{{\n\t\t\t\trow.get_formatted(\"amount\", doc) }}</td>\n\t\t</tr>\n\t\t{%- endfor -%}\n\t</tbody>\n</table>\n<div class = \"row\">\n    <div class = \"col-md-6\">Total</div>\n    <div class = \"col-md-6 text-right\">{{doc.total_amount}}</div>\n</div>",
